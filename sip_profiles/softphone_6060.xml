<profile name="softphone_6060">
  <!-- http://wiki.freeswitch.org/wiki/Sofia_Configuration_Files -->
  <!-- This profile is only for outbound registrations to providers -->
  <settings>
    <param name="sip-port" value="6060"/>
    <param name="context" value="fsDialer"/>
    <param name="ext-rtp-ip" value="$${local_ip_v4}"/>
    <param name="ext-sip-ip" value="$${local_ip_v4}"/>
    <param name="rtp-ip" value="$${local_ip_v4}"/>
    <param name="sip-ip" value="$${local_ip_v4}"/>

    <param name="user-agent-string" value="pstn_6070"/>
    <param name="NDLB-allow-crypto-in-avp" value="true"/>

    <param name="debug" value="8"/>
    <!-- If you want FreeSWITCH to shutdown if this profile fails to load, uncomment the next line. -->
    <!-- <param name="shutdown-on-fail" value="true"/> -->
    <param name="sip-trace" value="yes"/>
    <param name="sip-capture" value="yes"/>
    <param name="rfc2833-pt" value="101"/>
    <!-- RFC 5626 : Send reg-id and sip.instance -->
    <!--<param name="enable-rfc-5626" value="true"/> -->
    <param name="dialplan" value="XML"/>
    <param name="dtmf-duration" value="2000"/>
    <param name="liberal-dtmf" value="true"/>
    <!-- <param name="inbound-codec-prefs" value="PCMU"/> -->
    <param name="inbound-codec-prefs" value="GSM, PCMU, opus"/>
    <param name="outbound-codec-prefs" value="GSM, PCMU, opus"/>
    <!-- <param name="outbound-codec-prefs" value="PCMU"/> -->
    <param name="hold-music" value="$${hold_music}"/>
    <param name="rtp-timer-name" value="soft"/>
    <!--<param name="enable-100rel" value="true"/>-->
    <!--<param name="disable-srv503" value="true"/>-->
    <!-- This could be set to "passive" -->
    <param name="local-network-acl" value="localnet.auto"/>
    <param name="manage-presence" value="false"/>
    <param name="inbound-late-negotiation" value="true"/>
    <param name="renegotiate-codec-on-reinvite" value="true"/>

    <!--<param name="aggressive-nat-detection" value="true"/>-->
    <param name="inbound-codec-negotiation" value="generous"/>
    <param name="outbound-codec-negotiation" value="generous"/>
    <param name="nonce-ttl" value="60"/>
    <param name="auth-calls" value="false"/>
    <!-- <param name="inbound-late-negotiation" value="true"/> -->
    <!-- <param name="inbound-zrtp-passthru" value="true"/> --> <!-- (also enables late negotiation) -->

    <param name="rtp-timeout-sec" value="300"/>
    <param name="rtp-hold-timeout-sec" value="1800"/>
    <!--<param name="enable-3pcc" value="true"/>-->
  </settings>
</profile>
